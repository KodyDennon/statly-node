{
  "name": "@statly/observe-sdk",
  "version": "0.1.0",
  "description": "JavaScript SDK for Statly Observe - Error tracking and monitoring",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run"
  },
  "keywords": [
    "error-tracking",
    "monitoring",
    "observability",
    "statly",
    "sdk"
  ],
  "author": "Statly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/statly"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=16.0.0"
  }
}
